{
  "name": "app-shuttle-control-server",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/backend/app-shuttle-control-server/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/backend/app-shuttle-control-server",
        "main": "apps/backend/app-shuttle-control-server/src/main.ts",
        "tsConfig": "apps/backend/app-shuttle-control-server/tsconfig.app.json",
        "assets": ["apps/backend/app-shuttle-control-server/src/assets"],
        "webpackConfig": "apps/backend/app-shuttle-control-server/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {
          "generateLockfile": true
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "app-shuttle-control-server:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "app-shuttle-control-server:build:development"
        },
        "production": {
          "buildTarget": "app-shuttle-control-server:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/backend/app-shuttle-control-server/jest.config.ts"
      }
    },
    "docker-build": {
      "dependsOn": ["build"],
      "command": "docker build -f apps/backend/app-shuttle-control-server/Dockerfile . -t app-shuttle-control-server"
    }
  },
  "tags": []
}
